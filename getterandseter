public class Main
{
	public static void main(String[] args) {
		Student s1=new Student();
		s1.setName("mogit");
		System.out.println(s1.getName());
		s1.setAge(4);
		System.out.println(s1.getAge());
		}
}
class Student{
    String name;
    int rollno;
    int age;
    public void setName(String name){
        this.name=name;
    }
    public String getName(){
        return name;
    }
    public void setAge(int age){
        this.age=age;
    }
    public int getAge(){
        return age;
    } 
}